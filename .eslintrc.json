{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-restricted-globals": ["error", "event", "fdescribe"],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_urlSplitter",
            "_urlCombiner",
            "_openCache",
            "_fetchRequest",
            "_addCache",
            "_toggleDrawer",
            "_closeDrawer",
            "_likeButtonContainer",
            "_restaurant",
            "_renderButton",
            "_isRestaurantExist",
            "_renderLiked",
            "_renderLike",
            "_button",
            "_drawer",
            "_content",
            "_initialAppShell"
          ]
        }
      ]
    }
  }
  